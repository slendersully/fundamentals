#!/usr/bin/env node
"use strict"

var c = require('skilstak-colors')
function printPlain() {
  
  console.log("Hello World");
}
var printColor = function (name) {

  console.log(c.b + name)

}
var printMulti = function (name) {
  
  console.log(c.y + name)
}

var firstArg = process.argv[2] 

printMulti(firstArg);

printColor("troy is boring");
printMulti("troy is not as cool as me");




